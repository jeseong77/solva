{
  "expo": {
    "name": "solva",
    "slug": "solva",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/solva-main.png",
    "scheme": "solva",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeseong77.solva",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["https://", "http"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/solva-main.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/solva-main.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "페르소나의 프로필과 커버 사진을 설정하기 위해 앨범 접근 권한이 필요해요."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "02a2706d-484b-4605-b6bf-de1a8b97450c"
      }
    }
  }
}
